    "docs:build": "vitepress build components",
    "docs:serve": "vitepress serve components",
    "prepare": "husky install"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "lint-staged": "^10.5.4",
    "normalize.css": "^8.0.1",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "vite": "^2.2.2",
    "vitepress": "^0.13.0",
    "vue": "^3.0.5",
    "vue-router": "^4.0.6",
    "vue-tsc": "^0.0.15"
  },
  "husky": {
    "hooks": {
